<%= render 'homes/list', book: @book, user: current_user %>

<div class='col-xs-9'>
  <h2>Users</h2>
  <table class='table table-hover table-inverse'>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
      </tr>
    </thead>
    <% @users.each do |user| %>
    <tbody>
      <td><%= attachment_image_tag user, :profile_image, :fill, 50, 50, format: 'jpeg' %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user_path(user) %></td>
    <% end %>
    </tbody>
  </table>
</div>