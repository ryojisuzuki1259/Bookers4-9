<%= render 'homes/list', book: @book, user: @user, books: @books %>

<h2>Users</h2>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>

  <% @users.each do |user| %>
  <tbody>
    <td><%= attachment_image_tag user, :profile_image, :fill, 50, 50, format: 'jpeg' %></td>
    <td><%= user.name %></td>
    <td><%= link_to "Show", user_path(user) %></td>
  <% end %>
  </tbody>

</table>